{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "description": "A production ready NestJS boilerplate with batteries included. No Kidding!",
  "author": "Vinayak Sarawagi <vinayaksarawagi25@gmail.com>",
  "private": false,
  "license": "MIT",
  "keywords": [
    "nestjs-boilerplate",
    "nestjs-objection",
    "nestjs-starter-template",
    "nestjs-template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/squareboat/nestjs-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/squareboat/nestjs-boilerplate/issues"
  },
  "homepage": "https://github.com/squareboat/nestjs-boilerplate",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "nest start --debug --watch",
    "app": "nest start $app --watch",
    "start:all:dev": "pm2 start dev.ecosystem.config.js --env development",
    "start:all:prod": "pm2 start prod.ecosystem.config.js --env production",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs-boilerplate/test/jest-e2e.json",
    "knex": "knex --knexfile knexfile.ts",
    "seed:run": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make -- -x ts"
  },
  "dependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/throttler": "^4.0.0",
    "@sentry/node": "^7.37.2",
    "@sentry/tracing": "^7.37.2",
    "@squareboat/nest-console": "^1.0.9",
    "@squareboat/nest-events": "^0.1.0",
    "@squareboat/nest-queue-strategy": "^0.0.9",
    "@squareboat/nestjs-localization": "^0.0.4",
    "@squareboat/nestjs-objection": "^0.1.7",
    "@types/nodemailer": "^6.4.7",
    "aws-sdk": "^2.1304.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ejs": "^3.1.8",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "html-minifier": "^4.0.0",
    "ioredis": "^5.3.0",
    "mjml": "^4.13.0",
    "moment": "^2.29.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "query-string": "^7.0.1",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.1",
    "rxjs": "^7.8.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.15",
    "@types/html-minifier": "^4.0.2",
    "@types/jest": "^29.2.6",
    "@types/lodash": "^4.14.191",
    "@types/mjml": "^4.7.0",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/pg": "^8.6.6",
    "@types/query-string": "^6.3.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "@libs/boat/(.*)": "<rootDir>/libs/boat/src/$1",
      "@libs/boat": "<rootDir>/libs/boat/src",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/users(|/.*)$": "<rootDir>/libs/users/src/$1",
      "^@libs/jobs(|/.*)$": "<rootDir>/libs/jobs/src/$1",
      "^@app/@application(|/.*)$": "<rootDir>/libs/@application/src/$1"
    }
  }
}
