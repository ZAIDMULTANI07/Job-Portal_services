<mjml>
    <mj-head>
        <mj-attributes>
            <mj-all font-family="Raleway" href="https://fonts.googleapis.com/css?family=Raleway" />
        </mj-attributes>

        <mj-style>
                <%- include('./font.css'); %>
                <%- include('./style.css'); %>
        </mj-style>
    </mj-head>

    <mj-body css-class="default-bg">
        <mj-section>
            <mj-column>
                <mj-spacer height="10px"></mj-spacer>
            </mj-column>
        </mj-section>
        <mj-section css-class="header">
            <mj-column>
                <mj-image src="https://qyubic-services.s3.us-east-2.amazonaws.com/originals/logos/Qyubic-Final-Logo-23rd-June-2021-1.svg" alt="Engagr Logo"
                    align="center" width="200px" padding="0px">
                </mj-image>
            </mj-column>
        </mj-section>

        <mj-section background-color="#ffffff">